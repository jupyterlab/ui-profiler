{
  "title": "Style Rule Benchmark Options",
  "type": "object",
  "properties": {
    "repeats": {
      "title": "Number of repeats",
      "type": "integer",
      "minimum": 1,
      "default": 3
    },
    "skipPattern": {
      "title": "Regular expression to filter out rules",
      "type": "string",
      "default": "(fa-|Icon|bp3|mod-hidden)"
    },
    "includePattern": {
      "title": "Regular expression to filter rules to include",
      "type": "string",
      "default": ""
    }
  }
}
